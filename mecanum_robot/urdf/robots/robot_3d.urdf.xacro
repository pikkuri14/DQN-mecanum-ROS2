    <xacro:include filename="$(find mecanum_robot)/urdf/control/mec_mobile.gazebo"/>

  <link name="base_link">
    <inertial>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <mass
        value="1.76013710919286" />
      <inertia
        ixx="0.00423041439249349"
        ixy="-2.31393934415601E-06"
        ixz="-7.7385064447188E-05"
        iyy="0.00452988705418676"
        iyz="-1.77534456626722E-05"
        izz="0.00785365615805233" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="file://$(find mecanum_robot)/meshes/base_link.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.752941176470588 0.752941176470588 0.752941176470588 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="file://$(find mecanum_robot)/meshes/base_link.STL" />
      </geometry>
    </collision>
  </link>
    <!-- <xacro:robot_3d_base prefix="$(arg prefix)"/> -->

    <xacro:mecanum_wheel
      prefix="$(arg prefix)"
      side="front_left"
      x_reflect="1"
      y_reflect="1"/>

    <xacro:mecanum_wheel
      prefix="$(arg prefix)"
      side="front_right"
      x_reflect="1"
      y_reflect="-1"/>

    <xacro:mecanum_wheel
      prefix="$(arg prefix)"
      side="back_left"
      x_reflect="-1"
      y_reflect="1"/>

    <xacro:mecanum_wheel
      prefix="$(arg prefix)"
      side="back_right"
      x_reflect="-1"
      y_reflect="-1"/>


