<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="robot_3d" xmlns:gz="http://gazebosim.org/schema">

    <xacro:property name="M_PI" value="3.1415926535897931" />

    <xacro:arg name="robot_name" default="robot_3d"/>
    <xacro:arg name="prefix" default=""/>
    <xacro:arg name="use_gazebo" default="false"/>

    <xacro:include filename="$(find mecanum_robot)/urdf/mech/robot_3d_base.urdf.xacro"/>
    <xacro:include filename="$(find mecanum_robot)/urdf/mech/mecanum_wheel.urdf.xacro"/>
    <xacro:include filename="$(find mecanum_robot)/urdf/sensors/rgbd_camera.urdf.xacro"/>
    <!-- <xacro:include filename="$(find mecanum_robot)/urdf/sensors/lidar.urdf.xacro"/>
